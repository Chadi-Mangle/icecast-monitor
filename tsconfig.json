{
  "compilerOptions": {
    "target": "ESNext",
    "module": "NodeNext",
    "strict": true,
    "verbatimModuleSyntax": true,
    "skipLibCheck": true,
    "types": [
      "node"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx",
    "outDir": "./dist",
    "rootDir": "./src",
    "baseUrl": "./",
    "paths": {
      "#env": ["src/env.ts"],
      "#types": ["src/types/index.ts"],
      "#utils/*": ["src/utils/*"]
    }
  },
  "exclude": ["node_modules"]
}
